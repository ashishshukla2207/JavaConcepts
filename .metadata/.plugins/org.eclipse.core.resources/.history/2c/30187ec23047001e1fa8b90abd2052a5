package javamethods;

public class MethodsInJava {
    
	
	//no input no return
	
	public void test() {
		System.out.println("test method");
	}
	
	public static void main(String[] args) {
	
		//create object : java will look for obj in main method
		
		MethodsInJava obj= new MethodsInJava();
		obj.test();

	}

}
